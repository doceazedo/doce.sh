{
	"name": "doceazedo.com",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "concurrently -n svelte,pocketbase -p name -c bgYellow,bgCyan 'pnpm vite dev' './pocketbase/pocketbase serve'",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"typegen": "pocketbase-typegen --env -o src/lib/pocketbase-types.ts"
	},
